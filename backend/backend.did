type Username = text;
type Streak = nat;
type Result_1 = 
 variant {
   err: text;
   ok: text;
 };
type Result = 
 variant {
   err: text;
   ok;
 };
service : {
  checkIn: () -> (Result_1);
  generateQRCode: () -> (text) query;
  getLeaderboard: () -> (vec record {
                               Username;
                               Streak;
                             }) query;
  setUsername: (text) -> (Result);
}
